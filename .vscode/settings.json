// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude":{
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/*.js": { "when":  "$(basename).ts" },
        "**/*.js.map": true,
        "dist": true
    }
}
